{
  "name": "mainmatter-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --cache --fix",
    "lint:md": "prettier --check '**/*md'",
    "lint:md:fix": "prettier --write '**/*md'",
    "lint": "npm-run-all --print-name --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --print-name --aggregate-output --continue-on-error --parallel \"lint:*:fix\"",
    "build": "cross-env ELEVENTY_ENV=production run-s build:sass build:js build:html",
    "build:html": "eleventy",
    "build:js": "rollup --config ./rollup.config.js",
    "build:sass": "sass src/assets/css/app.scss ./assets/css/app.css --style=compressed",
    "debug": "DEBUG=Eleventy* eleventy",
    "js": "rollup --config ./rollup.config.js --watch",
    "watch": "cross-env ELEVENTY_ENV=development && run-p sass js serve",
    "sass": "sass src/assets/css/app.scss ./assets/css/app.css --style=compressed --watch",
    "serve": "eleventy  --quiet --serve --port=8080",
    "dev": "npm run watch"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-img": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@11ty/eleventy-plugin-rss": "1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "4.1.0",
    "@quasibit/eleventy-plugin-sitemap": "^2.1.5",
    "@tbranyen/jsdom": "13.0.0",
    "cross-env": "7.0.3",
    "dayjs": "^1.10.4",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "html-minifier": "4.0.0",
    "markdown-it": "13.0.1",
    "netlify-plugin-11ty": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rollup": "^2.40.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.32.8",
    "slugify": "1.6.5",
    "svgo": "^2.7.0",
    "wicg-inert": "^3.1.1"
  },
  "browserslist": [
    "last 1 Chrome versions",
    "last 1 Firefox versions",
    "last 1 Safari versions",
    "last 1 Edge versions"
  ],
  "dependencies": {
    "lax.js": "^2.0.3"
  },
  "engines": {
    "node": ">=16"
  },
  "volta": {
    "node": "16.17.0",
    "yarn": "1.22.19"
  }
}
