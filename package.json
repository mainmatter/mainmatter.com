{
  "name": "mainmatter-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --cache --fix",
    "lint:md": "prettier --check '**/*md'",
    "lint:md:fix": "prettier --write '**/*md'",
    "lint:sass": "prettier --check '**/*scss'",
    "lint:sass:fix": "prettier --write '**/*scss'",
    "lint:njk": "prettier --check '**/*njk'",
    "lint:njk:fix": "prettier --write '**/*njk'",
    "lint": "npm-run-all --print-name --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --print-name --aggregate-output --continue-on-error --parallel \"lint:*:fix\"",
    "build": "cross-env ELEVENTY_ENV=production run-s build:sass build:js build:html",
    "build:html": "eleventy",
    "build:js": "rollup --config ./rollup.config.mjs",
    "build:sass": "sass src/assets/css/app.scss ./dist/assets/css/app.css --style=compressed",
    "debug": "DEBUG=Eleventy* eleventy",
    "js": "rollup --config ./rollup.config.mjs --watch",
    "watch": "cross-env ELEVENTY_ENV=development && run-p sass js serve",
    "sass": "sass src/assets/css/app.scss ./dist/assets/css/app.css --style=compressed --watch",
    "serve": "eleventy --incremental --quiet --serve --port=8080",
    "dev": "npm run watch",
    "percy:build": "run-s build percy:disable-lazy-images",
    "percy:disable-lazy-images": "replace-in-file --configFile=.percy-disable-lazy-loading.js",
    "percy": "percy snapshot",
    "crawl": "node scripts/crawl.js",
    "this-week": "twios/this-week-in-open-source --config-path=config/this-week-in-open-source.config.json"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-img": "^6.0.0",
    "@11ty/eleventy-navigation": "^1.0.0",
    "@11ty/eleventy-plugin-rss": "2.0.3",
    "@11ty/eleventy-plugin-syntaxhighlight": "5.0.0",
    "@lhci/cli": "^0.14.0",
    "@percy/cli": "^1.28.7",
    "@quasibit/eleventy-plugin-schema": "github:mainmatter/eleventy-plugin-schema#service",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@tbranyen/jsdom": "13.0.0",
    "broken-link-checker": "^0.7.8",
    "colors": "^1.4.0",
    "cross-env": "7.0.3",
    "dayjs": "^1.10.4",
    "dotenv": "^17.2.3",
    "eleventy-plugin-og-image": "^4.0.1",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-n": "^17.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "express": "^5.0.0",
    "html-minifier": "4.0.0",
    "markdown-it": "14.1.0",
    "markdown-it-footnote": "^4.0.0",
    "netlify-plugin-11ty": "^1.1.0",
    "npm-run-all2": "^7.0.0",
    "prettier": "3.4.2",
    "prettier-plugin-jinja-template": "^2.0.0",
    "replace-in-file": "^8.0.0",
    "rollup": "^4.18.0",
    "sass": "^1.32.8",
    "slugify": "1.6.6",
    "svgo": "^3.0.0",
    "wicg-inert": "^3.1.1"
  },
  "browserslist": [
    "last 1 Chrome versions",
    "last 1 Firefox versions",
    "last 1 Safari versions",
    "last 1 Edge versions"
  ],
  "dependencies": {
    "@sentry/browser": "^9.0.0",
    "gsap": "^3.12.3",
    "lax.js": "^2.0.3"
  },
  "engines": {
    "node": ">=20"
  },
  "volta": {
    "node": "22.15.0",
    "pnpm": "9.15.4"
  },
  "packageManager": "pnpm@9.15.4"
}
