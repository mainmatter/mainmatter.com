{
  "name": "mainmatter-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --cache --fix",
    "lint:md": "prettier --check '**/*md'",
    "lint:md:fix": "prettier --write '**/*md'",
    "lint": "npm-run-all --print-name --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --print-name --aggregate-output --continue-on-error --parallel \"lint:*:fix\"",
    "build": "cross-env ELEVENTY_ENV=production run-s build:sass build:js build:html",
    "build:html": "eleventy",
    "build:js": "rollup --config ./rollup.config.js",
    "build:sass": "run-p build:sass:app build:sass:workshop-lp",
    "build:sass:app": "sass src/assets/css/app.scss ./assets/css/app.css --style=compressed",
    "build:sass:workshop-lp": "sass src/assets/css/workshop-lp.scss ./assets/css/workshop-lp.css --style=compressed",
    "debug": "DEBUG=Eleventy* eleventy",
    "js": "rollup --config ./rollup.config.js --watch",
    "watch": "cross-env ELEVENTY_ENV=development && run-p sass js serve",
    "sass": "run-p sass:app sass:workshop-lp",
    "sass:app": "sass src/assets/css/app.scss ./assets/css/app.css --style=compressed --watch",
    "sass:workshop-lp": "sass src/assets/css/workshop-lp.scss ./assets/css/workshop-lp.css --style=compressed --watch",
    "serve": "eleventy  --quiet --serve --port=8080",
    "dev": "npm run watch",
    "percy:build": "run-s build percy:disable-lazy-images",
    "percy:disable-lazy-images": "replace-in-file --configFile=.percy-disable-lazy-loading.js",
    "percy": "percy snapshot",
    "crawl": "node scripts/crawl.js",
    "gravityci": "gravityci",
    "this-week": "twios/this-week-in-open-source --config-path=config/this-week-in-open-source.config.json"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-img": "^3.0.0",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@11ty/eleventy-plugin-rss": "1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "5.0.0",
    "@lhci/cli": "^0.12.0",
    "@percy/cli": "^1.10.2",
    "@quasibit/eleventy-plugin-sitemap": "^2.1.5",
    "@tbranyen/jsdom": "13.0.0",
    "broken-link-checker": "^0.7.8",
    "colors": "^1.4.0",
    "cross-env": "7.0.3",
    "dayjs": "^1.10.4",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.1",
    "html-minifier": "4.0.0",
    "markdown-it": "13.0.2",
    "netlify-plugin-11ty": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "replace-in-file": "^7.0.0",
    "rollup": "^2.40.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.32.8",
    "slugify": "1.6.6",
    "svgo": "^3.0.0",
    "wicg-inert": "^3.1.1"
  },
  "browserslist": [
    "last 1 Chrome versions",
    "last 1 Firefox versions",
    "last 1 Safari versions",
    "last 1 Edge versions"
  ],
  "dependencies": {
    "@sentry/browser": "^7.16.0",
    "lax.js": "^2.0.3"
  },
  "engines": {
    "node": ">=16"
  },
  "volta": {
    "node": "18.14.1",
    "yarn": "1.22.19"
  }
}
